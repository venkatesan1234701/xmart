<form action="/user/checkout" method="POST" id="checkoutForm">

  <!-- Address Selection -->
  <div class="my-6">
    <div class="md:flex gap-6 flex-wrap">
      <% if (addresses && addresses.length > 0) { %>
        <% addresses.forEach((address, index) => { %>
          <div class="lg:w-1/2 w-full">
            <div class="card card-body flex-col gap-4 relative">
              
              <!-- Address radio -->
              <div class="relative flex items-center gap-2">
                <input
                  class="w-4 h-4 text-green-600 bg-white border-gray-300 rounded-full focus:ring-green-600 focus:outline-none focus:ring-2"
                  type="radio"
                  name="selectedAddress"
                  value="<%= address._id %>"
                  <%= index === 0 ? "checked" : "" %>
                  
                />
                <label class="text-gray-800 inline-block"><%= address.addressType %></label>
              </div>

              <!-- Address content -->
              <address class="not-italic mt-1">
                <strong><%= address.firstName %></strong><br/>
                <%= address.addressLine1 %><br/>
                <% if (address.addressLine2) { %>
                  <%= address.addressLine2 %><br/>
                <% } %>
                <%= address.city %>, <%= address.state %>, <%= address.country %> - <%= address.zipCode %><br/>
              </address>

              <!-- Default address label -->
              <% if (address.isDefault) { %>
                <span class="text-red-600 font-medium">Default address</span>
              <% } %>

       

            </div>
          </div>
        <% }) %>
      <% } else { %>
        <p>No saved addresses yet. 
          <a href="/user/add-address" class="text-blue-600 hover:underline">Add one</a>
        </p>
      <% } %>
    </div>
  </div>

  <!-- Payment Methods -->
  <div class="py-4">
    <h3 class="font-bold text-gray-900 mb-3">Payment Method</h3>

