<script>
document.addEventListener("DOMContentLoaded", () => {

  // Auto check every 4 seconds
  setInterval(checkStockUpdates, 4000);

  async function checkStockUpdates() {
    try {
      const res = await fetch('/cart/check-latest-stock');
      const data = await res.json();

      if (!data.success) return;

      // If stock changed for any product
      if (data.forceUpdate) {
        Swal.fire({
          icon: "warning",
          title: "Stock Updated",
          text: "Product quantity changed. Cart updated automatically!"
        }).then(() => location.reload());
      }

    } catch (err) {
      console.log("Auto stock check failed:", err);
    }
  }

});


  const dropdownBtn = document.getElementById("dropdownButton");
  const dropdown = document.getElementById("couponDropdown");

  dropdownBtn.addEventListener("click", (e) => {
    e.stopPropagation();
    dropdown.classList.toggle("hidden");
  });

  document.addEventListener("click", () => {
    dropdown.classList.add("hidden");
  });




document.addEventListener("DOMContentLoaded", () => {
  
  const checkoutBtn = document.querySelector("a[href='/checkout']");

  checkoutBtn.addEventListener("click", async (e) => {
    e.preventDefault(); // stop default navigation

    try {
      const res = await fetch('/cart/validate-before-checkout', {
        method: 'GET'
      });

      const data = await res.json();

      if (!data.success) {
        Swal.fire({
          icon: "warning",
          title: "Stock Changed",
          text: data.message
        }).then(() => {
          location.reload(); // refresh cart to show updated quantity
        });
        return;
      }

      // Everything is fine â†’ Proceed to checkout
      window.location.href = "/checkout";

    } catch (err) {
      Swal.fire({
        icon: "error",
        title: "Server Error",
        text: "Please try again later."
      });
    }
  });

});


document.getElementById("cancelCoupon").classList.remove("hidden");


document.addEventListener('DOMContentLoaded', () => {
  const couponForm = document.getElementById('couponForm');
  const couponMessage = document.getElementById('couponMessage');
  const couponDiscountValue = document.getElementById('couponDiscountValue');
  const grandTotalValue = document.getElementById('grandTotalValue');
  const grandTotalButton = document.getElementById('grandTotalButton');

  couponForm.addEventListener('submit', async (e) => {
    e.preventDefault();
    const formData = new FormData(couponForm);
    const couponCode = formData.get('couponCode');

    try {
      const response = await fetch('/apply-coupon', {
        method: 'POST',
        headers: { 'Content-Type': 'application/json' },
        body: JSON.stringify({ couponCode })
      });
      const data = await response.json();

      if (data.success) {
        couponMessage.textContent = data.message;
        couponMessage.className = 'text-green-600 mt-2 text-sm';
        couponDiscountValue.textContent = `-â‚¹${data.discount.toFixed(2)}`;
        grandTotalValue.textContent = `â‚¹${data.newGrandTotal.toFixed(2)}`;
        grandTotalButton.textContent = `â‚¹${data.newGrandTotal.toFixed(2)}`;

        // â­ SHOW CANCEL BUTTON
        document.getElementById("cancelCoupon").classList.remove("hidden");

      } else {
        couponMessage.textContent = data.message;
        couponMessage.className = 'text-red-600 mt-2 text-sm';
        couponDiscountValue.textContent = '-â‚¹0.00';

        const subtotal = parseFloat(document.getElementById('subtotalValue').textContent.replace('â‚¹',''));
        const shipping = parseFloat(document.getElementById('serviceFeeValue').textContent.replace('â‚¹',''));
        const originalTotal = subtotal + shipping;

        grandTotalValue.textContent = `â‚¹${originalTotal.toFixed(2)}`;
        grandTotalButton.textContent = `â‚¹${originalTotal.toFixed(2)}`;
      }
    } catch (err) {
      console.error('Error applying coupon:', err);
      couponMessage.textContent = 'Server error applying coupon';
      couponMessage.className = 'text-red-600 mt-2 text-sm';
    }
  });
});


