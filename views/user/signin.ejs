<script>
document.addEventListener("DOMContentLoaded", () => {
  const signinForm = document.querySelector('form.needs-validation');
  const emailInput = document.getElementById('formSigninEmail');
  const passwordInput = document.getElementById('formSigninPassword');
  const togglePassword = document.getElementById('togglePassword');

  const emailError = emailInput.parentElement.querySelector('.invalid-feedback');
  const passwordError = passwordInput.parentElement.querySelector('.invalid-feedback');

  // üëÅ Toggle Password Show / Hide (WORKS 100%)
  togglePassword.addEventListener('click', () => {
    if (passwordInput.type === 'password') {
      passwordInput.type = 'text';
      togglePassword.textContent = 'üôà';
    } else {
      passwordInput.type = 'password';
      togglePassword.textContent = 'üëÅ';
    }
  });

  // ---------- Error Message Logic ----------
  function showError(errorEl) {
    errorEl.style.display = "block";
  }

  function hideError(errorEl) {
    errorEl.style.display = "none";
  }

  // Typing time hide error
  emailInput.addEventListener("input", () => hideError(emailError));
  passwordInput.addEventListener("input", () => hideError(passwordError));

  // ---------- Form Submit ----------
  signinForm.addEventListener('submit', async (e) => {
    e.preventDefault();

    let valid = true;

    if (!emailInput.value.trim()) {
      showError(emailError);
      valid = false;
    }

    if (!passwordInput.value.trim()) {
      showError(passwordError);
      valid = false;
    }

    if (!valid) return;

    try {
      const res = await fetch('/signin', {
        method: 'POST',
        headers: { 'Content-Type': 'application/json' },
        body: JSON.stringify({
          email: emailInput.value.trim(),
          password: passwordInput.value.trim()
        })
      });

      const data = await res.json();

      // Blocked user
      if (data.blocked) {
        Swal.fire({
          title: 'Account Blocked',
          text: data.msg,
          icon: 'error'
        });
        return;
      }

      // Unverified user
      if (data.verify === false) {
        const { value: action } = await Swal.fire({
          title: 'Account not verified',
          html: data.msg,
          icon: 'warning',
          showCancelButton: true,
          confirmButtonText: 'Resend verification',
          cancelButtonText: 'Close'
        });

        if (action) {
          const r = await fetch('/resend-verification', {
            method: 'POST',
            headers: { 'Content-Type': 'application/json' },
            body: JSON.stringify({ email: emailInput.value.trim() })
          });
          const rr = await r.json();
          Swal.fire({
            title: rr.ok ? 'Sent' : 'Error',
            text: rr.msg,
            icon: rr.ok ? 'success' : 'error'
          });
        }
        return;
      }


      
if (res.googleUser) {
  Swal.fire({
    icon: "info",
    title: "Google Account Found",
    text: res.msg,
    confirmButtonText: "Login with Google"
  }).then(() => {
    window.location.href = "/auth/google";
  });
  return;
}


      // Login success
      if (data.ok && data.verify) {
        Swal.fire({
          icon: 'success',
          title: 'Login Successful',
          text: 'Welcome ' + (data.name || ''),
          timer: 2000,
          showConfirmButton: false
        }).then(() => {
          window.location.href = data.redirect || '/';
        });
        return;
      }

      // Other errors
      Swal.fire({
        title: 'Login Failed',
        text: data.msg,
        icon: 'error'
      });

    } catch (err) {
      Swal.fire({
        title: 'Error',
        text: 'Server error',
        icon: 'error'
      });
    }
  });
});


</script>



