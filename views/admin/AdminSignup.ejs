	/* Reset & Base */
body {
  font-family: "Inter", sans-serif;
  background: #f8fafc;
  margin: 0;
  padding: 0;
}

/* Login Card Wrapper */
.login-wrapper {
  max-width: 420px;
  margin: 70px auto;
  padding: 40px;
  background: #ffffff;
  border-radius: 18px;
  box-shadow: 0px 10px 25px rgba(0, 0, 0, 0.07);
  animation: fadeIn 0.5s ease-in-out;
}

/* Title */
.login-wrapper h1 {
  font-size: 22px;
  font-weight: 700;
  color: #111827;
  text-align: center;
  margin-bottom: 10px;
}

.login-wrapper p {
  text-align: center;
  margin-bottom: 25px;
  color: #6b7280;
  font-size: 15px;
}

/* Input Style */
.form-control {
  border: 1px solid #cbd5e1;
  padding: 12px 14px;
  border-radius: 10px;
  width: 100%;
  transition: 0.2s ease;
  font-size: 15px;
}

.form-control:focus {
  border-color: #22c55e;
  box-shadow: 0px 0px 0px 3px rgba(34, 197, 94, 0.25);
  outline: none;
}

/* Password Eye Icon */
.passwordToggler {
  position: absolute;
  right: 12px;
  top: 12px;
  cursor: pointer;
  color: #6b7280;
  font-size: 20px;
  transition: 0.2s ease;
}

.passwordToggler:hover {
  color: #111827;
}

/* Button */
.btn-login {
  background: #16a34a;
  color: #fff;
  padding: 12px;
  border-radius: 10px;
  width: 100%;
  font-size: 16px;
  font-weight: 600;
  transition: 0.2s ease;
}

.btn-login:hover {
  background: #15803d;
  transform: translateY(-2px);
  box-shadow: 0px 5px 12px rgba(34, 197, 94, 0.3);
}

/* Error Box */
.error-box {
  background: #fee2e2;
  color: #b91c1c;
  border: 1px solid #fecaca;
  padding: 12px;
  border-radius: 8px;
  margin-bottom: 10px;
  font-size: 14px;
}

/* Animation */
@keyframes fadeIn {
  from { opacity: 0; transform: translateY(8px); }
  to { opacity: 1; transform: translateY(0); }
}

</style>
